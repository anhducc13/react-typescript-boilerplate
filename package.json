{
  "name": "teko-admin-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^5.6.4",
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.3",
    "@types/uuid": "^7.0.0",
    "antd": "^4.0.0",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "craco-less": "^1.17.0",
    "darkreader": "^4.9.17",
    "history": "^4.10.1",
    "i18next": "^19.1.0",
    "node-sass": "^4.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-i18next": "^11.3.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "react-tracker-teko": "^3.1.4",
    "sass-resources-loader": "^2.0.1",
    "teko-oauth2": "^1.3.1",
    "typescript": "~3.7.2",
    "uuid": "^7.0.1"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --testPathIgnorePatterns 'jira.test.ts' -u",
    "test:cov": "craco test --coverage -u --testPathIgnorePatterns 'jira.test.ts' --watchAll=false",
    "test:jira": "craco test jira.test.ts -u --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/**.{js,jsx,ts,tsx}",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "storybook-docs": "start-storybook --docs",
    "build-storybook-docs": "build-storybook --docs"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@storybook/addon-actions": "^6.0.12",
    "@storybook/addon-essentials": "^6.0.12",
    "@storybook/addon-links": "^6.0.12",
    "@storybook/node-logger": "^6.0.12",
    "@storybook/preset-ant-design": "^0.0.1",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.12",
    "@testing-library/dom": "^5.0.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "babel-loader": "^8.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.6",
    "prettier": "^1.19.1",
    "react-is": "^16.13.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {}
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/index.{ts,tsx}",
      "!**/*stories.tsx",
      "!src/services/apis/*.ts",
      "!src/**/jira*.ts",
      "!src/serviceWorker.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 70,
        "statements": 50
      }
    }
  }
}
